{
    "path": {
        "scss/style.scss": {
            "sass.enabled": true,
            "sass.options": {
                "output": "../css/style.css",
                "includePaths": [],
                "imagePath": null,
                "sourceComments": "map",
                "outputStyle": "compressed"
            },
            "debug.showErrorsInStatusBar": true
        },
        "scss/_*": {
            "sass.enabled": false
        },
        "scss/includes/*": {
            "sass.enabled": false
        }
    }
}