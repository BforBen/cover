{
    "path": {
        "sass/style.scss": {
            "sass.enabled": true,
            "sass.options": {
                "output": "../css/style.css",
                "includePaths": [],
                "imagePath": null,
				"sourceComments": "map",
				"sourceMap": "style.css.map",
                "outputStyle": "compressed"
            }
        },
		"sass/iDangerous/*": {
			"sass.enabled": false
		},
		"sass/FontAwesome/*": {
			"sass.enabled": false
		}
    }
}